package com.ilad.subsets;

public class SubSets {
	public static void subSets(int n, int k) throws Exception {
		if(n < 1 || n > 9 || k < n || k > 9) {
			throw new Exception("n or k are illegal!");
		}
		for(int j = 1; j <= k; ++j) {
			String num = "";
			int jToDigit = j + '0';
			char digit = (char)jToDigit;
		}
	}
	
	public static void main(String[] args) throws Exception {
		subSets(3, 5);
	}
}
