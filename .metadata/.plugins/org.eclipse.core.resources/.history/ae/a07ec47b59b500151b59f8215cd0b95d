	public SynchronizedCircularBufferWith2Mutexes(int memCapacity_) throws IllegalBlockSizeException {
		super(memCapacity_);
	}
	
	// Other Methods
	@Override
	synchronized public boolean read(E outerBuffer) {
		
	}

}
