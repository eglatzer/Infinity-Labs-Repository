package com.ilad.datefinder;

public class DateFinder {
	private int m_year;
	private Month m_month;
	private int m_day;

	static private Map<Integer,Integer> m_yearValueMap;
	private final static int[] daysInMonth;

	public DateFinder(int year_, int month_, int day_) {
		m_year = year_;
		m_month = month_;
		m_day = day_;
	}

	public boolean isLeapYear() {
		
	}

	public boolean isValidDate() {
		
	}

	public Day findDay() {

	}

	public DateFinder setDate(int year_, Month month_, int day_) {

	}

	public enum Day {
		SUNDAY(1), MONDAY(2), TUESDAY(3), WEDNESDAY(4), THURSDAY(5), FRIDAY(6), SATURDAY(7);
		private int value;

		private Day(int value) {
			this.value = value;

		}
	};

	public enum Month {
		JANUARY(1), FEBRUARY(2), MARCH(3), APRIL(4), MAY(5), JUNE(6), JULY(7), AUGUST(8), SEPTEMBER(9), OCTOBER(
				10), NOVEMBER(11), DECEMBER(12);

		private int value;

		private Month(int value) {
			this.value = value;

		}
	};
