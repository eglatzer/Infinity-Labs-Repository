package com.ilad.subsets;

public class SubSets {
	public static void subSets(int n, int k) throws Exception {
		if(n < 1 || n > 9 || k < n || k > 9) {
			throw new Exception("n or k are illegal!");
		}
		for(int i = 1; i <= k; ++i) {
			String num = "";
			int iToDigit = i + '0';
			char digit = (char)iToDigit;
			System.out.println(digit);
			for(int j = 1; j <= n; ++j) {
				
			}
		}
	}
	
	public static void main(String[] args) throws Exception {
		subSets(3, 5);
	}
}
