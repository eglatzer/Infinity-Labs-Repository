package com.ilad.subsets;

public class SubSets {
	public static void subsets(int n, int k) throws Exception {
		if(n < 1 || n > 9 || k < n || k > 9) {
			throw new Exception("n or k are illegal!");
		}
		for(int i = 1; i <= k; ++i) {
			String num = "";
			int i_to_digit = i + '0';
			for(int j = 1; j <= n; ++j) {
				if(1 == j || 0 == num.length() || i > num.charAt(j - 2) - '0') {
					num = num + i_to_digit;
				}
			}
			if(n - 1 == num.length()) {
				System.out.print(num + " ");
			}
		}
	}
}
